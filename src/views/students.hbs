<div class="container">
  <h1>Student Details</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Gender</th>
        <th scope="col">Age</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      {{#each data}}
      <tr>
        <th scope="row">{{this.studentId}}</th>
        <td>{{this.studentName}}</td>
        <td>{{this.gender}}</td>
        <td>{{this.age}}</td>
        <td><a href="/edit-student/{{this.teacherId}}/students/{{this.studentId}}">Edit </a> </td>
      </tr> {{/each}} </tbody>
  </table> <a class="btn btn-primary btn-lg" href="/add-student">Add Student</a>
</div>